            <input variable="sayi1"/>
            <output expression="&quot;L&#252;tfen &#304;kinci Say&#305;n&#305;z&#305; Giriniz&quot;" newline="True"/>
            <input variable="sayi2"/>
            <call expression="toplama(sayi1,sayi2)"/>
            <call expression="&#231;arpma(sayi1,sayi2)"/>
